<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="hotel_reservation_sequence" model="ir.sequence">
            <field name="name">Reservation Sequence</field>
            <field name="code">hotel.reservation</field>
            <field name="prefix">R</field>
            <field name="padding">5</field>
        </record>
        <record id="hotel_booking_number_sequence" model="ir.sequence">
            <field name="name">Booking Number Sequence</field>
            <field name="code">hotel.booking.number</field>
            <field name="prefix">B%(year)s%(month)s%(day)s</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>